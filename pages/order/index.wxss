page {
  background-color: #eeeff0;
}

.header {
  height: 122rpx;
  background-color: #000;
}

.tabs {
  display: flex;
  align-items: center;
  justify-content: space-around;
  height: 100%;
}

.tab {
  display: flex;
  height: 100%;
  align-items: center;
  border-bottom: 2px solid transparent;
}

.tab-icon {
  margin-right: 18rpx;
  width: 60rpx;
  height: 60rpx;
  border-radius: 8rpx;
  background-color: #646b70;
  background-repeat: no-repeat;
  background-size: 50%;
  background-position: center;
}

.icon-sun {
  background-image: url(data:image/png;base64,);
}

.icon-moon {
  background-image: url(data:image/png;base64,);
}

.tab-text {
  font-size: 30rpx;
  color: #646b70;
}

.tab-active {
  border-bottom: 2px solid #059983;
}

.tab-active .tab-text {
  color: #059983;
}

.tab-active .tab-icon {
  background-color: #059983;
}

.copies-title {
  padding: 0 20rpx;
  line-height: 80rpx;
}

.copies-title__prefix {
  font-size: 24rpx;
  color: #999;
}

.copies-title__total {
  margin-right: 10rpx;
  font-size: 30rpx;
  color: #059983;
}

.copies-title__suffix {
  font-size: 24rpx;
  color: #059983;
}

.copies-context {
  position: relative;
  display: flex;
  flex-wrap: wrap;
  background-color: #fff;
}

.copies-context::after {
  content: '';
  position: absolute;
  top: 0;
  left: 0;
  width: 200%;
  height: 200%;
  -webkit-transform: scale(.5);
  transform: scale(.5);
  -webkit-transform-origin: 0 0;
  transform-origin: 0 0;
  pointer-events: none;
  box-sizing: border-box;
  border: 0 solid #e5e5e5;
  border-top-width: 1px;
  border-bottom-width: 1px;
}

.copies-context__prefix {
  font-size: 28rpx;
  color: #333;
}

.copies-context__total {
  margin-right: 10rpx;
  font-size: 36rpx;
  color: #059983;
}

.copies-context__suffix {
  font-size: 24rpx;
  color: #059983;
}

.copies-context-item {
  position: relative;
  display: flex;
  align-items: center;
  justify-content: center;
  width: 50%;
  height: 100rpx;
}

.copies-context-item::after {
  content: '';
  position: absolute;
  top: 0;
  left: 0;
  width: 200%;
  height: 200%;
  -webkit-transform: scale(.5);
  transform: scale(.5);
  -webkit-transform-origin: 0 0;
  transform-origin: 0 0;
  pointer-events: none;
  box-sizing: border-box;
  border: 0 solid #e5e5e5;
  border-right-width: 1px;
  border-bottom-width: 1px;
}

.cell-group {
  margin-top: 20rpx;
  background-color: #fff;
}

.cell:last-child::after {
  display: none;
}

.cell {
  position: relative;
  display: flex;
  align-items: center;
  justify-content: space-between;
  margin: 0 20rpx;
  height: 82rpx;
}

.cell::after {
  content: '';
  position: absolute;
  top: 0;
  left: 0;
  width: 200%;
  height: 200%;
  -webkit-transform: scale(.5);
  transform: scale(.5);
  -webkit-transform-origin: 0 0;
  transform-origin: 0 0;
  pointer-events: none;
  box-sizing: border-box;
  border: 0 dashed #ddd;
  border-bottom-width: 1px;
  border-radius: 4px
}

.cell [class^=cell] {
  flex: 1;
  flex-wrap: nowrap;
  text-align: center;
}

.cell__date {
  flex: 1.5 !important;
  font-size: 24rpx;
  color: #999;
}

.cell__name {
  font-size: 26rpx;
  color: #666;
}

.cell__status {
  font-size: 28rpx;
  color: #059983;
}